---
layout: base.njk
eleventyComputed:
  title: "{{ seo.title }}"
  description: "{{ seo.description }}"
  metaImage: "{{ page.seo.fields.featured_image.fields.file.url }}"
  metaUrl: "{{ page.seo.fields.seo_link }}"
---
<article class="padding-both">
  <section class="article-main-block">
    <div class="content-wrapper">
      <h1>{{ page.title }}</h1>
      {{ content | safe }}
    </div>
  </section>
</article>