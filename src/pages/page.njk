---
layout: base.njk
pagination:
  data: collections.page
  size: 1
  alias: page
permalink: "/{{ page.title | cleanSlug }}/"
eleventyComputed:
  reviewPosts: "{{ page }}"
  title: "{{ page.title }}"
  description: "{{ page.seo.fields.description }}"
  metaImage: "{{ page.seo.fields.featured_image.fields.file.url }}"
  metaUrl: "{{ page.seo.fields.seo_link }}"
---
<article class="padding-both">
    <section class="article-main-block">
        <div class="content-wrapper">
            <h1>{{ page.title }}</h1>
            {{ page.pageContentHtml | safe }}
        </div>
    </section>
</article>