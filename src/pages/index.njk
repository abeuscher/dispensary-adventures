---
layout: base.njk
title: Home Page
permalink: /
---
{%- from "../macros/debugger.njk" import debugger -%}
<article>
    <div class="content-wrapper padding-top"></div>
    <div class="content-wrapper padding-bottom">
        <div class="home-blogroll" id="post-bucket">

            {% for post in collections.reviewPost %}

                <a class="blog-thumb" href="/entries/{{ post.slug | cleanSlug }}/">
                    <article class="inner">
                        <div class="thumb" data-bg="{{ post.seo.fields.featured_image.fields.file.url }}"></div>
                        <div class="content">
                            <h2>{{ post.title }}</h2>
                        </div>
                        <p>{{debugger(post.seo.fields.featured_image.fields)}}</p>
                        <div class="post-date">{{ post.date | date('MM/DD/YYYY') }}</div>
                    </article>
                </a>
            {% endfor %}
        </div>
    </div>
</article>